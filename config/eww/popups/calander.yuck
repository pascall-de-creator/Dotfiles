(defwidget calender_popup []
    (box
        :class "popup"
        "Calender Goes Here"
    )
)

(defwindow calender_menu
    :monitor 0
    :class "popup"
    :windowtype "dock"
    :wm-ignore false
    :stacking "fg"
    :exclusive false
    :geometry (geometry
        :x "10px"
        :y "10px"
        :width "350px"
        :height "300px"
        :anchor "top right"
    )
    (calender_popup)
)

